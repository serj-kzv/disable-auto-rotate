{
  "manifest_version": 2,
  "name": "disable-auto-rotate",
  "description": "Disable a page auto rotation.",
  "short_name": "disable-auto-rotate",
  "author": "serj-kzv",
  "version": "1.2",
  "homepage_url": "https://github.com/serj-kzv/disable-auto-rotate",
  "content_scripts": [
    {
      "js": [
        "/content/main.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "/web_accessible_resources/*"
  ]
}
